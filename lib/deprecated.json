{
  "generator-star": true,
  "global-strict": true,
  "max-depth": true,
  "max-len": true,
  "max-params": true,
  "max-statements": true,
  "no-bitwise": true,
  "no-comma-dangle": true,
  "no-empty-class": true,
  "no-extra-strict": true,
  "no-plusplus": true,
  "no-space-before-semi": true,
  "no-wrap-func": true,
  "space-after-function-name": true,
  "space-before-function-parentheses": true,
  "space-in-brackets": true,
  "space-unary-word-ops": true,
  "spaced-line-comment": true
}

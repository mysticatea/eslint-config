{
  "name": "eslint-config-mysticatea",
  "version": "6.0.0",
  "description": "Shareable configurations of ESLint for me.",
  "main": "es2015.js",
  "files": [
    "base.js",
    "browser.js",
    "es2015.js",
    "es5.js",
    "mocha.js",
    "modules.js",
    "node.js"
  ],
  "engines": {
    "node": "^4.0.0 || >=6.0.0"
  },
  "peerDependencies": {
    "eslint": ">=3.7.1",
    "eslint-plugin-eslint-comments": ">=0.1.1",
    "eslint-plugin-mysticatea": ">=4.0.0",
    "eslint-plugin-node": ">=2.1.2"
  },
  "dependencies": {
    "eslint": "^3.7.1",
    "eslint-plugin-eslint-comments": "^0.1.1",
    "eslint-plugin-mysticatea": "^4.0.0",
    "eslint-plugin-node": "^2.0.0",
    "globals": "^9.8.0"
  },
  "devDependencies": {
    "chokidar-cli": "^1.2.0",
    "eslint": "^3.4.0",
    "mocha": "^3.0.2"
  },
  "scripts": {
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "pretest": "eslint .",
    "test": "cd test && mocha rules.js --reporter progress",
    "watch": "chokidar \"*.js\" \"test/*.js\" --initial --command \"npm test\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mysticatea/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "author": "Toru Nagashima",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mysticatea/eslint-config/issues"
  },
  "homepage": "https://github.com/mysticatea/eslint-config#readme"
}
